{
  "$schema": "http://localhost:3001/schema/registry-item.json",
  "name": "navbar-dropdown",
  "type": "registry:block",
  "author": "axionjs (https://www.axionjs.com)",
  "description": "A navbar dropdown component.",
  "dependencies": [
    "motion"
  ],
  "registryDependencies": [
    "button",
    "dropdown-menu"
  ],
  "files": [
    {
      "path": "blocks/navbar-dropdown.tsx",
      "content": "\"use client\";\n\nimport { Button } from \"@/registry/default/ui/button\";\nimport {\n  DropdownMenu,\n  DropdownMenuContent,\n  DropdownMenuItem,\n  DropdownMenuLabel,\n  DropdownMenuSeparator,\n  DropdownMenuTrigger,\n} from \"@/registry/default/ui/dropdown-menu\";\nimport {\n  Moon,\n  Sun,\n  Menu,\n  X,\n  ChevronDown,\n  User,\n  Settings,\n  LogOut,\n} from \"lucide-react\";\nimport { useState } from \"react\";\nimport { motion, AnimatePresence } from \"motion/react\";\nimport Link from \"next/link\";\n\nexport default function NavbarDropdown() {\n  const [isOpen, setIsOpen] = useState(false);\n  const [theme, setTheme] = useState(\"light\");\n\n  // Logo component\n  const Logo = () => (\n    <div className=\"flex items-center gap-2\">\n      <svg\n        xmlns=\"http://www.w3.org/2000/svg\"\n        viewBox=\"0 0 24 24\"\n        fill=\"none\"\n        stroke=\"currentColor\"\n        strokeWidth=\"2\"\n        strokeLinecap=\"round\"\n        strokeLinejoin=\"round\"\n        className=\"h-6 w-6 text-primary\"\n      >\n        <circle cx=\"12\" cy=\"12\" r=\"10\" />\n        <path d=\"M12 2a4.5 4.5 0 0 0 0 9 4.5 4.5 0 0 1 0 9 10 10 0 0 0 0-18Z\" />\n        <path d=\"M12 2c5.5 0 10 4.5 10 10\" />\n      </svg>\n      <span className=\"font-bold text-lg\">Quantum</span>\n    </div>\n  );\n\n  // Navigation menu component with dropdowns\n  const NavMenu = ({ className = \"\" }) => (\n    <div className={className}>\n      <ul className=\"flex space-x-8\">\n        <li>\n          <Link\n            href=\"#\"\n            className=\"text-sm font-medium hover:text-primary transition-colors\"\n          >\n            Home\n          </Link>\n        </li>\n        <li>\n          <DropdownMenu>\n            <DropdownMenuTrigger className=\"flex items-center gap-1 text-sm font-medium hover:text-primary transition-colors\">\n              Products <ChevronDown className=\"h-4 w-4\" />\n            </DropdownMenuTrigger>\n            <DropdownMenuContent>\n              <DropdownMenuLabel>Our Products</DropdownMenuLabel>\n              <DropdownMenuSeparator />\n              <DropdownMenuItem>Analytics</DropdownMenuItem>\n              <DropdownMenuItem>Automation</DropdownMenuItem>\n              <DropdownMenuItem>Commerce</DropdownMenuItem>\n              <DropdownMenuItem>AI Assistant</DropdownMenuItem>\n            </DropdownMenuContent>\n          </DropdownMenu>\n        </li>\n        <li>\n          <DropdownMenu>\n            <DropdownMenuTrigger className=\"flex items-center gap-1 text-sm font-medium hover:text-primary transition-colors\">\n              Resources <ChevronDown className=\"h-4 w-4\" />\n            </DropdownMenuTrigger>\n            <DropdownMenuContent>\n              <DropdownMenuLabel>Resources</DropdownMenuLabel>\n              <DropdownMenuSeparator />\n              <DropdownMenuItem>Documentation</DropdownMenuItem>\n              <DropdownMenuItem>Tutorials</DropdownMenuItem>\n              <DropdownMenuItem>Blog</DropdownMenuItem>\n              <DropdownMenuItem>Community</DropdownMenuItem>\n            </DropdownMenuContent>\n          </DropdownMenu>\n        </li>\n        <li>\n          <Link\n            href=\"#\"\n            className=\"text-sm font-medium hover:text-primary transition-colors\"\n          >\n            Pricing\n          </Link>\n        </li>\n      </ul>\n    </div>\n  );\n\n  // Mobile navigation sheet\n  const NavigationSheet = () => (\n    <>\n      <Button variant=\"ghost\" size=\"icon\" onClick={() => setIsOpen(true)}>\n        <Menu className=\"h-5 w-5\" />\n        <span className=\"sr-only\">Open menu</span>\n      </Button>\n\n      <AnimatePresence>\n        {isOpen && (\n          <motion.div\n            className=\"fixed inset-0 z-50 bg-background\"\n            initial={{ opacity: 0, x: \"100%\" }}\n            animate={{ opacity: 1, x: 0 }}\n            exit={{ opacity: 0, x: \"100%\" }}\n            transition={{ duration: 0.3 }}\n          >\n            <div className=\"flex h-16 items-center justify-between px-4 sm:px-6 lg:px-8\">\n              <Logo />\n              <Button\n                variant=\"ghost\"\n                size=\"icon\"\n                onClick={() => setIsOpen(false)}\n              >\n                <X className=\"h-5 w-5\" />\n                <span className=\"sr-only\">Close menu</span>\n              </Button>\n            </div>\n            <div className=\"px-4 sm:px-6 lg:px-8 py-8\">\n              <motion.ul\n                className=\"flex flex-col space-y-6\"\n                initial=\"hidden\"\n                animate=\"visible\"\n                variants={{\n                  hidden: { opacity: 0 },\n                  visible: {\n                    opacity: 1,\n                    transition: {\n                      staggerChildren: 0.1,\n                    },\n                  },\n                }}\n              >\n                <motion.li\n                  variants={{\n                    hidden: { opacity: 0, x: -20 },\n                    visible: { opacity: 1, x: 0 },\n                  }}\n                >\n                  <Link\n                    href=\"#\"\n                    className=\"text-xl font-medium hover:text-primary transition-colors\"\n                    onClick={() => setIsOpen(false)}\n                  >\n                    Home\n                  </Link>\n                </motion.li>\n                <motion.li\n                  variants={{\n                    hidden: { opacity: 0, x: -20 },\n                    visible: { opacity: 1, x: 0 },\n                  }}\n                >\n                  <div className=\"text-xl font-medium\">Products</div>\n                  <ul className=\"mt-2 ml-4 space-y-2\">\n                    <li>\n                      <Link\n                        href=\"#\"\n                        className=\"text-base hover:text-primary transition-colors\"\n                        onClick={() => setIsOpen(false)}\n                      >\n                        Analytics\n                      </Link>\n                    </li>\n                    <li>\n                      <Link\n                        href=\"#\"\n                        className=\"text-base hover:text-primary transition-colors\"\n                        onClick={() => setIsOpen(false)}\n                      >\n                        Automation\n                      </Link>\n                    </li>\n                    <li>\n                      <Link\n                        href=\"#\"\n                        className=\"text-base hover:text-primary transition-colors\"\n                        onClick={() => setIsOpen(false)}\n                      >\n                        Commerce\n                      </Link>\n                    </li>\n                    <li>\n                      <Link\n                        href=\"#\"\n                        className=\"text-base hover:text-primary transition-colors\"\n                        onClick={() => setIsOpen(false)}\n                      >\n                        AI Assistant\n                      </Link>\n                    </li>\n                  </ul>\n                </motion.li>\n                <motion.li\n                  variants={{\n                    hidden: { opacity: 0, x: -20 },\n                    visible: { opacity: 1, x: 0 },\n                  }}\n                >\n                  <div className=\"text-xl font-medium\">Resources</div>\n                  <ul className=\"mt-2 ml-4 space-y-2\">\n                    <li>\n                      <Link\n                        href=\"#\"\n                        className=\"text-base hover:text-primary transition-colors\"\n                        onClick={() => setIsOpen(false)}\n                      >\n                        Documentation\n                      </Link>\n                    </li>\n                    <li>\n                      <Link\n                        href=\"#\"\n                        className=\"text-base hover:text-primary transition-colors\"\n                        onClick={() => setIsOpen(false)}\n                      >\n                        Tutorials\n                      </Link>\n                    </li>\n                    <li>\n                      <Link\n                        href=\"#\"\n                        className=\"text-base hover:text-primary transition-colors\"\n                        onClick={() => setIsOpen(false)}\n                      >\n                        Blog\n                      </Link>\n                    </li>\n                    <li>\n                      <Link\n                        href=\"#\"\n                        className=\"text-base hover:text-primary transition-colors\"\n                        onClick={() => setIsOpen(false)}\n                      >\n                        Community\n                      </Link>\n                    </li>\n                  </ul>\n                </motion.li>\n                <motion.li\n                  variants={{\n                    hidden: { opacity: 0, x: -20 },\n                    visible: { opacity: 1, x: 0 },\n                  }}\n                >\n                  <Link\n                    href=\"#\"\n                    className=\"text-xl font-medium hover:text-primary transition-colors\"\n                    onClick={() => setIsOpen(false)}\n                  >\n                    Pricing\n                  </Link>\n                </motion.li>\n              </motion.ul>\n              <div className=\"mt-12 flex flex-col space-y-4\">\n                <Button variant=\"outline\" onClick={() => setIsOpen(false)}>\n                  Sign In\n                </Button>\n                <Button onClick={() => setIsOpen(false)}>Sign Up</Button>\n              </div>\n            </div>\n          </motion.div>\n        )}\n      </AnimatePresence>\n    </>\n  );\n\n  // User dropdown menu\n  const UserMenu = () => (\n    <DropdownMenu>\n      <DropdownMenuTrigger asChild>\n        <Button variant=\"ghost\" size=\"icon\" className=\"rounded-full\">\n          <User className=\"h-5 w-5\" />\n        </Button>\n      </DropdownMenuTrigger>\n      <DropdownMenuContent align=\"end\">\n        <DropdownMenuLabel>My Account</DropdownMenuLabel>\n        <DropdownMenuSeparator />\n        <DropdownMenuItem>\n          <User className=\"mr-2 h-4 w-4\" />\n          <span>Profile</span>\n        </DropdownMenuItem>\n        <DropdownMenuItem>\n          <Settings className=\"mr-2 h-4 w-4\" />\n          <span>Settings</span>\n        </DropdownMenuItem>\n        <DropdownMenuSeparator />\n        <DropdownMenuItem>\n          <LogOut className=\"mr-2 h-4 w-4\" />\n          <span>Log out</span>\n        </DropdownMenuItem>\n      </DropdownMenuContent>\n    </DropdownMenu>\n  );\n\n  return (\n    <div className=\"min-h-screen bg-muted\">\n      <motion.nav\n        className=\"h-16 bg-background border-b sticky top-0 z-40\"\n        initial={{ y: -100 }}\n        animate={{ y: 0 }}\n        transition={{ type: \"spring\", stiffness: 300, damping: 30 }}\n      >\n        <div className=\"h-full flex items-center justify-between max-w-screen-xl mx-auto px-4 sm:px-6 lg:px-8\">\n          <div className=\"flex items-center gap-8\">\n            <Logo />\n\n            {/* Desktop Menu */}\n            <NavMenu className=\"hidden md:block\" />\n          </div>\n\n          <div className=\"flex items-center gap-3\">\n            <motion.div\n              initial={{ opacity: 0, scale: 0.8 }}\n              animate={{ opacity: 1, scale: 1 }}\n              transition={{ delay: 0.2 }}\n              className=\"hidden sm:block\"\n            >\n              <Button variant=\"outline\" className=\"hidden sm:inline-flex\">\n                Sign In\n              </Button>\n            </motion.div>\n            <motion.div\n              initial={{ opacity: 0, scale: 0.8 }}\n              animate={{ opacity: 1, scale: 1 }}\n              transition={{ delay: 0.3 }}\n            >\n              <Button>Sign Up</Button>\n            </motion.div>\n\n            <motion.div\n              initial={{ opacity: 0 }}\n              animate={{ opacity: 1 }}\n              transition={{ delay: 0.4 }}\n              className=\"hidden sm:block\"\n            >\n              <UserMenu />\n            </motion.div>\n\n            <motion.div\n              initial={{ opacity: 0, rotate: -30 }}\n              animate={{ opacity: 1, rotate: 0 }}\n              transition={{ delay: 0.5 }}\n              whileHover={{ rotate: 180 }}\n            >\n              <Button\n                size=\"icon\"\n                variant=\"outline\"\n                onClick={() => setTheme(theme === \"light\" ? \"dark\" : \"light\")}\n              >\n                {theme === \"light\" ? (\n                  <Moon className=\"h-5 w-5\" />\n                ) : (\n                  <Sun className=\"h-5 w-5\" />\n                )}\n              </Button>\n            </motion.div>\n\n            {/* Mobile Menu */}\n            <div className=\"md:hidden\">\n              <NavigationSheet />\n            </div>\n          </div>\n        </div>\n      </motion.nav>\n    </div>\n  );\n}\n",
      "type": "registry:component",
      "target": ""
    }
  ]
}