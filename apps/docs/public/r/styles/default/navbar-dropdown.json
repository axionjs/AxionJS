      "path": "blocks/navbar-dropdown.tsx",
      "content": "\"use client\";\r\n\r\nimport { Button } from \"@/registry/default/ui/button\";\r\nimport {\r\n  DropdownMenu,\r\n  DropdownMenuContent,\r\n  DropdownMenuItem,\r\n  DropdownMenuLabel,\r\n  DropdownMenuSeparator,\r\n  DropdownMenuTrigger,\r\n} from \"@/registry/default/ui/dropdown-menu\";\r\nimport {\r\n  Moon,\r\n  Sun,\r\n  Menu,\r\n  X,\r\n  ChevronDown,\r\n  User,\r\n  Settings,\r\n  LogOut,\r\n} from \"lucide-react\";\r\nimport { useState } from \"react\";\r\nimport { motion, AnimatePresence } from \"motion/react\";\r\nimport Link from \"next/link\";\r\n\r\nexport default function NavbarDropdown() {\r\n  const [isOpen, setIsOpen] = useState(false);\r\n  const [theme, setTheme] = useState(\"light\");\r\n\r\n  // Logo component\r\n  const Logo = () => (\r\n    <div className=\"flex items-center gap-2\">\r\n      <svg\r\n        xmlns=\"http://www.w3.org/2000/svg\"\r\n        viewBox=\"0 0 24 24\"\r\n        fill=\"none\"\r\n        stroke=\"currentColor\"\r\n        strokeWidth=\"2\"\r\n        strokeLinecap=\"round\"\r\n        strokeLinejoin=\"round\"\r\n        className=\"h-6 w-6 text-primary\"\r\n      >\r\n        <circle cx=\"12\" cy=\"12\" r=\"10\" />\r\n        <path d=\"M12 2a4.5 4.5 0 0 0 0 9 4.5 4.5 0 0 1 0 9 10 10 0 0 0 0-18Z\" />\r\n        <path d=\"M12 2c5.5 0 10 4.5 10 10\" />\r\n      </svg>\r\n      <span className=\"font-bold text-lg\">Quantum</span>\r\n    </div>\r\n  );\r\n\r\n  // Navigation menu component with dropdowns\r\n  const NavMenu = ({ className = \"\" }) => (\r\n    <div className={className}>\r\n      <ul className=\"flex space-x-8\">\r\n        <li>\r\n          <Link\r\n            href=\"#\"\r\n            className=\"text-sm font-medium hover:text-primary transition-colors\"\r\n          >\r\n            Home\r\n          </Link>\r\n        </li>\r\n        <li>\r\n          <DropdownMenu>\r\n            <DropdownMenuTrigger className=\"flex items-center gap-1 text-sm font-medium hover:text-primary transition-colors\">\r\n              Products <ChevronDown className=\"h-4 w-4\" />\r\n            </DropdownMenuTrigger>\r\n            <DropdownMenuContent>\r\n              <DropdownMenuLabel>Our Products</DropdownMenuLabel>\r\n              <DropdownMenuSeparator />\r\n              <DropdownMenuItem>Analytics</DropdownMenuItem>\r\n              <DropdownMenuItem>Automation</DropdownMenuItem>\r\n              <DropdownMenuItem>Commerce</DropdownMenuItem>\r\n              <DropdownMenuItem>AI Assistant</DropdownMenuItem>\r\n            </DropdownMenuContent>\r\n          </DropdownMenu>\r\n        </li>\r\n        <li>\r\n          <DropdownMenu>\r\n            <DropdownMenuTrigger className=\"flex items-center gap-1 text-sm font-medium hover:text-primary transition-colors\">\r\n              Resources <ChevronDown className=\"h-4 w-4\" />\r\n            </DropdownMenuTrigger>\r\n            <DropdownMenuContent>\r\n              <DropdownMenuLabel>Resources</DropdownMenuLabel>\r\n              <DropdownMenuSeparator />\r\n              <DropdownMenuItem>Documentation</DropdownMenuItem>\r\n              <DropdownMenuItem>Tutorials</DropdownMenuItem>\r\n              <DropdownMenuItem>Blog</DropdownMenuItem>\r\n              <DropdownMenuItem>Community</DropdownMenuItem>\r\n            </DropdownMenuContent>\r\n          </DropdownMenu>\r\n        </li>\r\n        <li>\r\n          <Link\r\n            href=\"#\"\r\n            className=\"text-sm font-medium hover:text-primary transition-colors\"\r\n          >\r\n            Pricing\r\n          </Link>\r\n        </li>\r\n      </ul>\r\n    </div>\r\n  );\r\n\r\n  // Mobile navigation sheet\r\n  const NavigationSheet = () => (\r\n    <>\r\n      <Button variant=\"ghost\" size=\"icon\" onClick={() => setIsOpen(true)}>\r\n        <Menu className=\"h-5 w-5\" />\r\n        <span className=\"sr-only\">Open menu</span>\r\n      </Button>\r\n\r\n      <AnimatePresence>\r\n        {isOpen && (\r\n          <motion.div\r\n            className=\"fixed inset-0 z-50 bg-background\"\r\n            initial={{ opacity: 0, x: \"100%\" }}\r\n            animate={{ opacity: 1, x: 0 }}\r\n            exit={{ opacity: 0, x: \"100%\" }}\r\n            transition={{ duration: 0.3 }}\r\n          >\r\n            <div className=\"flex h-16 items-center justify-between px-4 sm:px-6 lg:px-8\">\r\n              <Logo />\r\n              <Button\r\n                variant=\"ghost\"\r\n                size=\"icon\"\r\n                onClick={() => setIsOpen(false)}\r\n              >\r\n                <X className=\"h-5 w-5\" />\r\n                <span className=\"sr-only\">Close menu</span>\r\n              </Button>\r\n            </div>\r\n            <div className=\"px-4 sm:px-6 lg:px-8 py-8\">\r\n              <motion.ul\r\n                className=\"flex flex-col space-y-6\"\r\n                initial=\"hidden\"\r\n                animate=\"visible\"\r\n                variants={{\r\n                  hidden: { opacity: 0 },\r\n                  visible: {\r\n                    opacity: 1,\r\n                    transition: {\r\n                      staggerChildren: 0.1,\r\n                    },\r\n                  },\r\n                }}\r\n              >\r\n                <motion.li\r\n                  variants={{\r\n                    hidden: { opacity: 0, x: -20 },\r\n                    visible: { opacity: 1, x: 0 },\r\n                  }}\r\n                >\r\n                  <Link\r\n                    href=\"#\"\r\n                    className=\"text-xl font-medium hover:text-primary transition-colors\"\r\n                    onClick={() => setIsOpen(false)}\r\n                  >\r\n                    Home\r\n                  </Link>\r\n                </motion.li>\r\n                <motion.li\r\n                  variants={{\r\n                    hidden: { opacity: 0, x: -20 },\r\n                    visible: { opacity: 1, x: 0 },\r\n                  }}\r\n                >\r\n                  <div className=\"text-xl font-medium\">Products</div>\r\n                  <ul className=\"mt-2 ml-4 space-y-2\">\r\n                    <li>\r\n                      <Link\r\n                        href=\"#\"\r\n                        className=\"text-base hover:text-primary transition-colors\"\r\n                        onClick={() => setIsOpen(false)}\r\n                      >\r\n                        Analytics\r\n                      </Link>\r\n                    </li>\r\n                    <li>\r\n                      <Link\r\n                        href=\"#\"\r\n                        className=\"text-base hover:text-primary transition-colors\"\r\n                        onClick={() => setIsOpen(false)}\r\n                      >\r\n                        Automation\r\n                      </Link>\r\n                    </li>\r\n                    <li>\r\n                      <Link\r\n                        href=\"#\"\r\n                        className=\"text-base hover:text-primary transition-colors\"\r\n                        onClick={() => setIsOpen(false)}\r\n                      >\r\n                        Commerce\r\n                      </Link>\r\n                    </li>\r\n                    <li>\r\n                      <Link\r\n                        href=\"#\"\r\n                        className=\"text-base hover:text-primary transition-colors\"\r\n                        onClick={() => setIsOpen(false)}\r\n                      >\r\n                        AI Assistant\r\n                      </Link>\r\n                    </li>\r\n                  </ul>\r\n                </motion.li>\r\n                <motion.li\r\n                  variants={{\r\n                    hidden: { opacity: 0, x: -20 },\r\n                    visible: { opacity: 1, x: 0 },\r\n                  }}\r\n                >\r\n                  <div className=\"text-xl font-medium\">Resources</div>\r\n                  <ul className=\"mt-2 ml-4 space-y-2\">\r\n                    <li>\r\n                      <Link\r\n                        href=\"#\"\r\n                        className=\"text-base hover:text-primary transition-colors\"\r\n                        onClick={() => setIsOpen(false)}\r\n                      >\r\n                        Documentation\r\n                      </Link>\r\n                    </li>\r\n                    <li>\r\n                      <Link\r\n                        href=\"#\"\r\n                        className=\"text-base hover:text-primary transition-colors\"\r\n                        onClick={() => setIsOpen(false)}\r\n                      >\r\n                        Tutorials\r\n                      </Link>\r\n                    </li>\r\n                    <li>\r\n                      <Link\r\n                        href=\"#\"\r\n                        className=\"text-base hover:text-primary transition-colors\"\r\n                        onClick={() => setIsOpen(false)}\r\n                      >\r\n                        Blog\r\n                      </Link>\r\n                    </li>\r\n                    <li>\r\n                      <Link\r\n                        href=\"#\"\r\n                        className=\"text-base hover:text-primary transition-colors\"\r\n                        onClick={() => setIsOpen(false)}\r\n                      >\r\n                        Community\r\n                      </Link>\r\n                    </li>\r\n                  </ul>\r\n                </motion.li>\r\n                <motion.li\r\n                  variants={{\r\n                    hidden: { opacity: 0, x: -20 },\r\n                    visible: { opacity: 1, x: 0 },\r\n                  }}\r\n                >\r\n                  <Link\r\n                    href=\"#\"\r\n                    className=\"text-xl font-medium hover:text-primary transition-colors\"\r\n                    onClick={() => setIsOpen(false)}\r\n                  >\r\n                    Pricing\r\n                  </Link>\r\n                </motion.li>\r\n              </motion.ul>\r\n              <div className=\"mt-12 flex flex-col space-y-4\">\r\n                <Button variant=\"outline\" onClick={() => setIsOpen(false)}>\r\n                  Sign In\r\n                </Button>\r\n                <Button onClick={() => setIsOpen(false)}>Sign Up</Button>\r\n              </div>\r\n            </div>\r\n          </motion.div>\r\n        )}\r\n      </AnimatePresence>\r\n    </>\r\n  );\r\n\r\n  // User dropdown menu\r\n  const UserMenu = () => (\r\n    <DropdownMenu>\r\n      <DropdownMenuTrigger asChild>\r\n        <Button variant=\"ghost\" size=\"icon\" className=\"rounded-full\">\r\n          <User className=\"h-5 w-5\" />\r\n        </Button>\r\n      </DropdownMenuTrigger>\r\n      <DropdownMenuContent align=\"end\">\r\n        <DropdownMenuLabel>My Account</DropdownMenuLabel>\r\n        <DropdownMenuSeparator />\r\n        <DropdownMenuItem>\r\n          <User className=\"mr-2 h-4 w-4\" />\r\n          <span>Profile</span>\r\n        </DropdownMenuItem>\r\n        <DropdownMenuItem>\r\n          <Settings className=\"mr-2 h-4 w-4\" />\r\n          <span>Settings</span>\r\n        </DropdownMenuItem>\r\n        <DropdownMenuSeparator />\r\n        <DropdownMenuItem>\r\n          <LogOut className=\"mr-2 h-4 w-4\" />\r\n          <span>Log out</span>\r\n        </DropdownMenuItem>\r\n      </DropdownMenuContent>\r\n    </DropdownMenu>\r\n  );\r\n\r\n  return (\r\n    <div className=\"min-h-screen bg-muted\">\r\n      <motion.nav\r\n        className=\"h-16 bg-background border-b sticky top-0 z-40\"\r\n        initial={{ y: -100 }}\r\n        animate={{ y: 0 }}\r\n        transition={{ type: \"spring\", stiffness: 300, damping: 30 }}\r\n      >\r\n        <div className=\"h-full flex items-center justify-between max-w-screen-xl mx-auto px-4 sm:px-6 lg:px-8\">\r\n          <div className=\"flex items-center gap-8\">\r\n            <Logo />\r\n\r\n            {/* Desktop Menu */}\r\n            <NavMenu className=\"hidden md:block\" />\r\n          </div>\r\n\r\n          <div className=\"flex items-center gap-3\">\r\n            <motion.div\r\n              initial={{ opacity: 0, scale: 0.8 }}\r\n              animate={{ opacity: 1, scale: 1 }}\r\n              transition={{ delay: 0.2 }}\r\n              className=\"hidden sm:block\"\r\n            >\r\n              <Button variant=\"outline\" className=\"hidden sm:inline-flex\">\r\n                Sign In\r\n              </Button>\r\n            </motion.div>\r\n            <motion.div\r\n              initial={{ opacity: 0, scale: 0.8 }}\r\n              animate={{ opacity: 1, scale: 1 }}\r\n              transition={{ delay: 0.3 }}\r\n            >\r\n              <Button>Sign Up</Button>\r\n            </motion.div>\r\n\r\n            <motion.div\r\n              initial={{ opacity: 0 }}\r\n              animate={{ opacity: 1 }}\r\n              transition={{ delay: 0.4 }}\r\n              className=\"hidden sm:block\"\r\n            >\r\n              <UserMenu />\r\n            </motion.div>\r\n\r\n            <motion.div\r\n              initial={{ opacity: 0, rotate: -30 }}\r\n              animate={{ opacity: 1, rotate: 0 }}\r\n              transition={{ delay: 0.5 }}\r\n              whileHover={{ rotate: 180 }}\r\n            >\r\n              <Button\r\n                size=\"icon\"\r\n                variant=\"outline\"\r\n                onClick={() => setTheme(theme === \"light\" ? \"dark\" : \"light\")}\r\n              >\r\n                {theme === \"light\" ? (\r\n                  <Moon className=\"h-5 w-5\" />\r\n                ) : (\r\n                  <Sun className=\"h-5 w-5\" />\r\n                )}\r\n              </Button>\r\n            </motion.div>\r\n\r\n            {/* Mobile Menu */}\r\n            <div className=\"md:hidden\">\r\n              <NavigationSheet />\r\n            </div>\r\n          </div>\r\n        </div>\r\n      </motion.nav>\r\n    </div>\r\n  );\r\n}\r\n",
      "type": "registry:component",
      "target": ""
    }
  ]
}