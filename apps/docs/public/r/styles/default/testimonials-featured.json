      "content": "\"use client\";\r\n\r\nimport { useRef, useEffect } from \"react\";\r\nimport {\r\n  Avatar,\r\n  AvatarFallback,\r\n  AvatarImage,\r\n} from \"@/registry/default/ui/avatar\";\r\nimport { Card, CardContent } from \"@/registry/default/ui/card\";\r\nimport { Badge } from \"@/registry/default/ui/badge\";\r\nimport { StarIcon } from \"lucide-react\";\r\nimport { motion, useInView, useAnimation } from \"motion/react\";\r\n\r\nexport default function TestimonialsFeatured() {\r\n  const ref = useRef(null);\r\n  const isInView = useInView(ref, { once: false, amount: 0.1 });\r\n  const controls = useAnimation();\r\n\r\n  useEffect(() => {\r\n    if (isInView) {\r\n      controls.start(\"visible\");\r\n    }\r\n  }, [controls, isInView]);\r\n\r\n  const containerVariants = {\r\n    hidden: { opacity: 0 },\r\n    visible: {\r\n      opacity: 1,\r\n      transition: {\r\n        staggerChildren: 0.1,\r\n        delayChildren: 0.2,\r\n      },\r\n    },\r\n  };\r\n\r\n  const itemVariants = {\r\n    hidden: { opacity: 0, y: 20 },\r\n    visible: {\r\n      opacity: 1,\r\n      y: 0,\r\n      transition: { duration: 0.5 },\r\n    },\r\n  };\r\n\r\n  return (\r\n    <section ref={ref} className=\"py-12 md:py-24\">\r\n      <div className=\"container px-4 md:px-6\">\r\n        <motion.div\r\n          className=\"mx-auto max-w-2xl text-center\"\r\n          initial={{ opacity: 0, y: 20 }}\r\n          animate={{ opacity: 1, y: 0 }}\r\n          transition={{ duration: 0.5 }}\r\n        >\r\n          <Badge variant=\"outline\" className=\"mb-4\">\r\n            Testimonials\r\n          </Badge>\r\n          <h2 className=\"text-3xl font-bold tracking-tight md:text-4xl\">\r\n            Loved by developers worldwide\r\n          </h2>\r\n          <p className=\"mt-4 text-muted-foreground\">\r\n            Our registry/new-york are used by thousands of developers to build\r\n            modern, accessible web applications.\r\n          </p>\r\n        </motion.div>\r\n\r\n        <motion.div\r\n          className=\"mx-auto mt-12 max-w-5xl\"\r\n          variants={containerVariants}\r\n          initial=\"hidden\"\r\n          animate={controls}\r\n        >\r\n          <div className=\"grid gap-6 md:grid-cols-2 lg:grid-cols-3\">\r\n            <motion.div variants={itemVariants} className=\"md:col-span-2\">\r\n              <Card className=\"h-full\">\r\n                <CardContent className=\"p-6\">\r\n                  <div className=\"flex items-start justify-between\">\r\n                    <Avatar className=\"h-14 w-14\">\r\n                      <AvatarImage\r\n                        src=\"https://randomuser.me/api/portraits/women/32.jpg\"\r\n                        alt=\"Emma Thompson\"\r\n                      />\r\n                      <AvatarFallback>ET</AvatarFallback>\r\n                    </Avatar>\r\n                    <div className=\"flex\">\r\n                      {Array(5)\r\n                        .fill(null)\r\n                        .map((_, i) => (\r\n                          <StarIcon\r\n                            key={i}\r\n                            className=\"h-5 w-5 fill-primary text-primary\"\r\n                          />\r\n                        ))}\r\n                    </div>\r\n                  </div>\r\n                  <div className=\"mt-4\">\r\n                    <blockquote className=\"text-lg font-medium\">\r\n                      \"Axion.js has completely transformed our development\r\n                      workflow. The registry/new-york are beautifully designed,\r\n                      accessible by default, and incredibly flexible. We've cut\r\n                      our development time in half while improving the quality\r\n                      of our UI.\"\r\n                    </blockquote>\r\n                    <div className=\"mt-6\">\r\n                      <p className=\"font-medium\">Emma Thompson</p>\r\n                      <p className=\"text-muted-foreground text-sm\">\r\n                        CTO at TechForward\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                </CardContent>\r\n              </Card>\r\n            </motion.div>\r\n\r\n            <motion.div variants={itemVariants}>\r\n              <Card className=\"h-full\">\r\n                <CardContent className=\"p-6\">\r\n                  <div className=\"flex items-start justify-between\">\r\n                    <Avatar className=\"h-10 w-10\">\r\n                      <AvatarImage\r\n                        src=\"https://randomuser.me/api/portraits/men/42.jpg\"\r\n                        alt=\"James Wilson\"\r\n                      />\r\n                      <AvatarFallback>JW</AvatarFallback>\r\n                    </Avatar>\r\n                    <div className=\"flex\">\r\n                      {Array(5)\r\n                        .fill(null)\r\n                        .map((_, i) => (\r\n                          <StarIcon\r\n                            key={i}\r\n                            className=\"h-4 w-4 fill-primary text-primary\"\r\n                          />\r\n                        ))}\r\n                    </div>\r\n                  </div>\r\n                  <div className=\"mt-4\">\r\n                    <blockquote className=\"text-sm\">\r\n                      \"As a solo developer, Axion.js has been a game-changer.\r\n                      The documentation is excellent, and the registry/new-york\r\n                      just work.\"\r\n                    </blockquote>\r\n                    <div className=\"mt-4\">\r\n                      <p className=\"text-sm font-medium\">James Wilson</p>\r\n                      <p className=\"text-muted-foreground text-xs\">\r\n                        Freelance Developer\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                </CardContent>\r\n              </Card>\r\n            </motion.div>\r\n\r\n            <motion.div\r\n              variants={itemVariants}\r\n              className=\"md:col-span-2 lg:col-span-1\"\r\n            >\r\n              <Card className=\"h-full\">\r\n                <CardContent className=\"p-6\">\r\n                  <div className=\"flex items-start justify-between\">\r\n                    <Avatar className=\"h-10 w-10\">\r\n                      <AvatarImage\r\n                        src=\"https://randomuser.me/api/portraits/women/52.jpg\"\r\n                        alt=\"Sophia Chen\"\r\n                      />\r\n                      <AvatarFallback>SC</AvatarFallback>\r\n                    </Avatar>\r\n                    <div className=\"flex\">\r\n                      {Array(5)\r\n                        .fill(null)\r\n                        .map((_, i) => (\r\n                          <StarIcon\r\n                            key={i}\r\n                            className=\"h-4 w-4 fill-primary text-primary\"\r\n                          />\r\n                        ))}\r\n                    </div>\r\n                  </div>\r\n                  <div className=\"mt-4\">\r\n                    <blockquote className=\"text-sm\">\r\n                      \"The accessibility features in Axion.js are exceptional.\r\n                      It's rare to find a component library that prioritizes\r\n                      a11y without compromising on design.\"\r\n                    </blockquote>\r\n                    <div className=\"mt-4\">\r\n                      <p className=\"text-sm font-medium\">Sophia Chen</p>\r\n                      <p className=\"text-muted-foreground text-xs\">\r\n                        UX Designer at DesignHub\r\n                      </p>\r\n                    </div>\r\n                  </div>\r\n                </CardContent>\r\n              </Card>\r\n            </motion.div>\r\n\r\n            <motion.div variants={itemVariants} className=\"lg:col-span-3\">\r\n              <Card>\r\n                <CardContent className=\"p-6\">\r\n                  <div className=\"grid gap-6 md:grid-cols-3\">\r\n                    <div className=\"md:col-span-1\">\r\n                      <div className=\"flex flex-col items-center justify-center md:items-start\">\r\n                        <div className=\"text-center md:text-left\">\r\n                          <p className=\"text-3xl font-bold\">4.9/5</p>\r\n                          <div className=\"mt-2 flex\">\r\n                            {Array(5)\r\n                              .fill(null)\r\n                              .map((_, i) => (\r\n                                <StarIcon\r\n                                  key={i}\r\n                                  className=\"h-5 w-5 fill-primary text-primary\"\r\n                                />\r\n                              ))}\r\n                          </div>\r\n                          <p className=\"mt-2 text-sm text-muted-foreground\">\r\n                            Based on 1,200+ reviews\r\n                          </p>\r\n                        </div>\r\n                      </div>\r\n                    </div>\r\n                    <div className=\"space-y-4 md:col-span-2\">\r\n                      <div className=\"flex items-center\">\r\n                        <div className=\"flex\">\r\n                          {Array(5)\r\n                            .fill(null)\r\n                            .map((_, i) => (\r\n                              <StarIcon\r\n                                key={i}\r\n                                className=\"h-4 w-4 fill-primary text-primary\"\r\n                              />\r\n                            ))}\r\n                        </div>\r\n                        <div className=\"ml-4 h-2 w-full rounded-full bg-muted\">\r\n                          <div\r\n                            className=\"h-2 rounded-full bg-primary\"\r\n                            style={{ width: \"92%\" }}\r\n                          ></div>\r\n                        </div>\r\n                        <span className=\"ml-2 text-sm font-medium\">92%</span>\r\n                      </div>\r\n                      <div className=\"flex items-center\">\r\n                        <div className=\"flex\">\r\n                          {Array(4)\r\n                            .fill(null)\r\n                            .map((_, i) => (\r\n                              <StarIcon\r\n                                key={i}\r\n                                className=\"h-4 w-4 fill-primary text-primary\"\r\n                              />\r\n                            ))}\r\n                          <StarIcon className=\"h-4 w-4 text-muted\" />\r\n                        </div>\r\n                        <div className=\"ml-4 h-2 w-full rounded-full bg-muted\">\r\n                          <div\r\n                            className=\"h-2 rounded-full bg-primary\"\r\n                            style={{ width: \"6%\" }}\r\n                          ></div>\r\n                        </div>\r\n                        <span className=\"ml-2 text-sm font-medium\">6%</span>\r\n                      </div>\r\n                      <div className=\"flex items-center\">\r\n                        <div className=\"flex\">\r\n                          {Array(3)\r\n                            .fill(null)\r\n                            .map((_, i) => (\r\n                              <StarIcon\r\n                                key={i}\r\n                                className=\"h-4 w-4 fill-primary text-primary\"\r\n                              />\r\n                            ))}\r\n                          {Array(2)\r\n                            .fill(null)\r\n                            .map((_, i) => (\r\n                              <StarIcon\r\n                                key={i}\r\n                                className=\"h-4 w-4 text-muted\"\r\n                              />\r\n                            ))}\r\n                        </div>\r\n                        <div className=\"ml-4 h-2 w-full rounded-full bg-muted\">\r\n                          <div\r\n                            className=\"h-2 rounded-full bg-primary\"\r\n                            style={{ width: \"2%\" }}\r\n                          ></div>\r\n                        </div>\r\n                        <span className=\"ml-2 text-sm font-medium\">2%</span>\r\n                      </div>\r\n                      <div className=\"flex items-center\">\r\n                        <div className=\"flex\">\r\n                          {Array(2)\r\n                            .fill(null)\r\n                            .map((_, i) => (\r\n                              <StarIcon\r\n                                key={i}\r\n                                className=\"h-4 w-4 fill-primary text-primary\"\r\n                              />\r\n                            ))}\r\n                          {Array(3)\r\n                            .fill(null)\r\n                            .map((_, i) => (\r\n                              <StarIcon\r\n                                key={i}\r\n                                className=\"h-4 w-4 text-muted\"\r\n                              />\r\n                            ))}\r\n                        </div>\r\n                        <div className=\"ml-4 h-2 w-full rounded-full bg-muted\">\r\n                          <div\r\n                            className=\"h-2 rounded-full bg-primary\"\r\n                            style={{ width: \"0%\" }}\r\n                          ></div>\r\n                        </div>\r\n                        <span className=\"ml-2 text-sm font-medium\">0%</span>\r\n                      </div>\r\n                      <div className=\"flex items-center\">\r\n                        <div className=\"flex\">\r\n                          <StarIcon className=\"h-4 w-4 fill-primary text-primary\" />\r\n                          {Array(4)\r\n                            .fill(null)\r\n                            .map((_, i) => (\r\n                              <StarIcon\r\n                                key={i}\r\n                                className=\"h-4 w-4 text-muted\"\r\n                              />\r\n                            ))}\r\n                        </div>\r\n                        <div className=\"ml-4 h-2 w-full rounded-full bg-muted\">\r\n                          <div\r\n                            className=\"h-2 rounded-full bg-primary\"\r\n                            style={{ width: \"0%\" }}\r\n                          ></div>\r\n                        </div>\r\n                        <span className=\"ml-2 text-sm font-medium\">0%</span>\r\n                      </div>\r\n                    </div>\r\n                  </div>\r\n                </CardContent>\r\n              </Card>\r\n            </motion.div>\r\n          </div>\r\n        </motion.div>\r\n      </div>\r\n    </section>\r\n  );\r\n}\r\n",
